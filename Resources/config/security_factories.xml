<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="bazinga.oauth.authentication.factory.class">Bazinga\OAuthBundle\DependencyInjection\Security\Factory\OAuthFactory</parameter>
    </parameters>

    <services>
        <service id="security.authentication.factory.bazinga_oauth"
            class="%bazinga.oauth.authentication.factory.class%" public="false">
            <tag name="security.listener.factory" />
        </service>
    </services>
</container>
